<body>
    <header id="header" >
        {{>header}}        
    </header>
    <div class="login-div-main App-div-main">
    <h2>OTP</h2>

		<form method="post" target="_blank">
			<div class="imgcontainer">
				<img src="img/logo.jpg" alt="login-img" class="login-img">
			</div>
		 
			<div class="container">
				<input type="text" placeholder="Enter OTP" name="Uname" required><br>
				<span style="color:red" class="pass_class"></span><br>
					
				<button type="button" onclick="location.href='/dashboard';">LogIn</button>
			</div>
			</div>
		</form>
    </div>
</body>
